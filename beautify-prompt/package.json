{
  "name": "beautify-prompt",
  "displayNa      "properties": {
        "beautify-prompt.beautificationMethod": {
          "type": "string",
          "default": "promptBoost",
          "enum": ["promptBoost", "simulation", "externalAI"],
          "enumDescriptions": [
            "Use VS Code's promptBoost API when available (recommended)",
            "Use simple rule-based simulation for beautification",
            "Use external AI service for more intelligent beautification"
          ],
          "description": "Method to use for beautifying prompts"
        },
        "beautify-prompt.aiService.apiKey": {
          "type": "string",
          "default": "",
          "description": "API key for the external AI service"
        },
        "beautify-prompt.aiService.endpoint": {
          "type": "string",
          "default": "https://api.openai.com/v1/chat/completions",
          "description": "Endpoint URL for the external AI service"
        },
        "beautify-prompt.aiService.model": {
          "type": "string",
          "default": "gpt-4",
          "description": "AI model to use for prompt beautification"
        }
      },fy Prompt for GitHub Copilot",
  "description": "Enhance and improve your GitHub Copilot prompts for better results in both editor and chat",
  "version": "0.0.2",
  "publisher": "C99-FHM-EMU-TrongPV6",
  "icon": "icons/beautify-prompt-icon.png",
  "author": {
    "name": "C99.FHM.EMU.TrongPV6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trongpv6/beautify-prompt.git"
  },
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "beautify-prompt.beautifyPrompt",
        "title": "Beautify Prompt",
        "icon": {
          "light": "icons/beautify-light.svg",
          "dark": "icons/beautify-dark.svg"
        }
      },
      {
        "command": "beautify-prompt.beautifyChatPrompt",
        "title": "Beautify Copilot Chat Prompt",
        "icon": {
          "light": "icons/beautify-chat-light.svg",
          "dark": "icons/beautify-chat-dark.svg"
        }
      }
    ],
    "configuration": {
      "title": "Beautify Prompt",
      "properties": {
        "beautify-prompt.beautificationMethod": {
          "type": "string",
          "default": "promptBoost",
          "enum": ["promptBoost", "simulation", "externalAI"],
          "enumDescriptions": [
            "Use VS Code's promptBoost API when available (recommended)",
            "Use simple rule-based simulation for beautification",
            "Use external AI service for more intelligent beautification"
          ],
          "description": "Method to use for beautifying prompts"
        },
        "beautify-prompt.aiService.apiKey": {
          "type": "string",
          "default": "",
          "description": "API key for the external AI service"
        },
        "beautify-prompt.aiService.endpoint": {
          "type": "string",
          "default": "https://api.openai.com/v1/chat/completions",
          "description": "Endpoint URL for the external AI service"
        },
        "beautify-prompt.aiService.model": {
          "type": "string",
          "default": "gpt-4",
          "description": "AI model to use for prompt beautification"
        }
      }
    },
    "menus": {
      "editor/context": [
        {
          "command": "beautify-prompt.beautifyPrompt",
          "when": "editorHasSelection",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "command": "beautify-prompt.beautifyPrompt",
          "when": "editorHasSelection",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "beautify-prompt.beautifyChatPrompt",
          "title": "Beautify GitHub Copilot Chat Prompt"
        }
      ],
      "webview/context": [
        {
          "command": "beautify-prompt.beautifyChatPrompt",
          "when": "webviewId =~ /^github.copilot.*/",
          "group": "navigation"
        }
      ],
      "webview/title": [
        {
          "command": "beautify-prompt.beautifyChatPrompt", 
          "when": "webviewId =~ /^github.copilot.*/",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "beautify-prompt.beautifyChatPrompt",
        "key": "ctrl+shift+b",
        "mac": "cmd+shift+b",
        "when": "webviewId =~ /^github.copilot.*/ && webviewFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "vsix-package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "ts-loader": "^9.5.2",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
